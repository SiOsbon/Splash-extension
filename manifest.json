{
  "manifest_version": 2,

  "name": "Daratus - Token projects",
  "description": "Productivity tool to get all the essential information about crypto projects in a single click.",
  "version": "0.9.0.22",
  "icons": {
    "128": "images/icon_128.png",
    "48": "images/icon_48.png",
    "16": "images/icon_16.png"
  },

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Daratus - Token projects"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://*.daratus.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}